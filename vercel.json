{
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "framework": "python",
  "functions": {
    "server.py": {
      "runtime": "python3.11"
    }
  },
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.py"
    },
    {
      "source": "/dashboard(.*)",
      "destination": "/server.py"
    }
  ]
}
